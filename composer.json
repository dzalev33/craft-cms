{
  "require": {
    "craftcms/cms": "^3.6.4",
    "craftcms/element-api": "2.6.0",
    "craftcms/feed-me": "4.3.6",
    "craftcms/redactor": "2.8.5",
    "jalendport/craft-fetch": "^1.3",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\yelpapimodule\\": "modules/yelpapimodule/src"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
